<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Automation" >

    <parameter name="suiteOwner" value="qpsdemo"/>
    <parameter name="jenkinsJobName" value="webtests"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsEmail" value="test@qaprosoft.com"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>

    <parameter name="jenkinsRegressionPipeline" value="Carina-Demo-Regression-Pipeline"/>
    <parameter name="jenkinsRegressionMatrix" value="browser=chrome,browser=firefox"/>
    <parameter name="jenkinsJobExecutionOrder" value="2"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>

    <test name="EcommerceTests" >
        <classes>
            <class name="com.solvd.ecommerce.EcommerceWebTest" />
        </classes>
    </test>

</suite>